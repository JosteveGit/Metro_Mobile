<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.metro.metromobile.dashboard.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.metro.metromobile.dashboard.ui.cars.CarFragment"
        android:label="Cars"
        tools:layout="@layout/fragment_cars" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.metro.metromobile.dashboard.ui.profile.ProfilesFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profiles" />
    <fragment
        android:id="@+id/navigation_history"
        android:name="com.metro.metromobile.dashboard.ui.bookings.BookingFragment"
        android:label="Bookings"
        tools:layout="@layout/fragment_booking" />
</navigation>